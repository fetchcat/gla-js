{
  "name": "backend",
  "version": "1.1.0",
  "description": "GLA-JS Backend",
  "main": "server.js",
  "scripts": {
    "server": "pm2 start server.js --name GLA-JS",
    "start": "node server.js",
    "deploy": "cd ../frontend && yarn build && cd ../backend && yarn server",
    "dev": "concurrently \"yarn start\" \"cd ../frontend && yarn serve\"",
    "node-install": "cd ../frontend && yarn install && cd ../backend && yarn install"
  },
