  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.1.0",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "mongoose": "^6.1.8",
    "pm2": "^5.2.0"
  }
}
